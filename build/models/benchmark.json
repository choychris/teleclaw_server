{
  "name": "benchmark",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "required": true
    },
    "created": {
      "type": "date",
      "required": true
    },
    "costRange": {
      "type": "object",
      "required": true
    },
    "overheadCost": {
      "type": "number",
      "required": true,
      "default": 1.5
    },
    "marginRate": {
      "type": "number",
      "required": true
    },
    "gamePlayRate": {
      "type": "number",
      "required": true
    },
    "realValuePerCoin": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "products": {
      "type": "hasMany",
      "model": "product",
      "foreginKey": "productId",
      "primaryKey": "id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "teleClawAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "teleClawAdmin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
