{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "required": true
    },
    "created": {
      "type": "date",
      "required": true
    },
    "lastLogIn": {
      "type": "date",
      "required": true
    },
    "logInStatus": {
      "type": "boolean"
    },
    "lastUpdated": {
      "type": "date"
    },
    "name":{
      "type": "string"
    },
    "username": {
      "type": "string"
    },
    "phone": {
      "type": "integer"
    },
    "address": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "password": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "language": {
      "type": "string",
      "required": true,
      "default": "en"
    },
    "counrtyCode": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "roles": {
      "type": "hasMany",
      "model": "role",
      "foreignKey": "principalId",
      "through": "rolemap"
    },
    "userIdentities": {
      "type": "hasMany",
      "model": "userIdentity",
      "foreignKey": "userId"
    },
    "transactions": {
      "type": "hasMany",
      "model": "transaction",
      "foreignKey": "userId"
    },
    "wallet": {
      "type": "hasOne",
      "model": "wallet",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "auth"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "checkTokenValid"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "accessTokens"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": [
        "findOne",
        "findById",
        "update"
      ]
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": [
        "delete",
        "find"
      ]
    }
  ],
  "methods": {}
}
