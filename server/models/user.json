{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "required": true
    },
    "create": {
      "type": "date",
      "required": true
    },
    "lastLogIn": {
      "type": "date",
      "required": true
    },
    "logInStatus": {
      "type": "boolean"
    },
    "lastUpdated": {
      "type": "date"
    },
    "name": {
      "type": "object"
    },
    "phone": {
      "type": "integer"
    },
    "address": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "password": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "language": {
      "type": "string",
      "required": true,
      "default": "en"
    },
    "counrtyCode": {
      "type": "string"
    },
    "3rdPartyAuth": {
      "type": "object"
    }
  },
  "validations": [],
  "relations": {
    "roles": {
      "type": "hasMany",
      "model": "role",
      "foreignKey": "principalId",
      "through": "rolemap"
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "auth"
    }
  ],
  "methods": {}
}
