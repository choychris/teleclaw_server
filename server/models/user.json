{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "indexes": {
    "code_index": {
      "keys": {
        "id": 1,
        "referral.code": 1
      }
    }
  },
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "required": true
    },
    "created": {
      "type": "date",
      "required": true
    },
    "lastLogIn": {
      "type": "date"
    },
    "lastUpdated": {
      "type": "date"
    },
    "name": {
      "type": "string"
    },
    "username": {
      "type": "string"
    },
    "phone": {
      "type": "number"
    },
    "address": {
      "type": "object"
    },
    "picture": {
      "type": "string"
    },
    "referral": {
      "code": "string",
      "isReferred": "boolean",
      "numOfReferred": "number"
    },
    "email": {
      "type": "string"
    },
    "Email":{
      "type": "string"
    },
    "password": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "language": {
      "type": "string",
      "required": true,
      "default": "en"
    },
    "preference": {
      "type": "object"
    },
    "counrtyCode": {
      "type": "string"
    },
    "bindedDevice": {
      "type": "array"
    }
  },
  "validations": [],
  "relations": {
    "roles": {
      "type": "hasMany",
      "model": "role",
      "foreignKey": "principalId",
      "through": "rolemap"
    },
    "userIdentities": {
      "type": "hasMany",
      "model": "userIdentity",
      "foreignKey": "userId"
    },
    "transactions": {
      "type": "hasMany",
      "model": "transaction",
      "foreignKey": "userId"
    },
    "deliveries": {
      "type": "hasMany",
      "model": "delivery",
      "foreignKey": "userId"
    },
    "wallet": {
      "type": "hasOne",
      "model": "wallet",
      "foreignKey": "userId"
    },
    "reservation": {
      "type": "hasOne",
      "model": "reservation",
      "foreignKey": "userId"
    },
    "paymentGateway": {
      "type": "hasOne",
      "model": "paymentGateway",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "auth"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "loginAdmin"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "pusherAuth"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "checkTokenValid"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "accessTokens"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": [
        "findById",
        "findOne",
        "__get__wallet",
        "__get__reservation",
        "__get__paymentGateway",
        "__get__transactions",
        "__get__deliveries",
        "__findById__accessTokens"
      ]
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "teleClawAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "teleClawAdmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "teleClawAdmin",
      "permission": "ALLOW",
      "property": "createAdmin"
    }
  ],
  "methods": {}
}
