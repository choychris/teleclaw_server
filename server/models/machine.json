{
  "name": "machine",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "injectOptionsFromRemoteContext": true,
  "properties": {
    "id": {
      "type": "string",
      "required": true
    },
    "created": {
      "type": "date",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true
    },
    "display": {
      "type": "boolean",
      "required": true
    },
    "reservation": {
      "type": "number",
      "required": true
    },
    "iotPlatorm": {
      "type": "object",
      "required": true
    },
    "player": {
      "type": "string",
      "required": true
    },
    "productId": {
      "type": "string",
      "required": true
    },
    "numOfPlay": {
      "type": "number",
      "required": true
    },
    "numOfSuccess": {
      "type": "number",
      "require": false
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
